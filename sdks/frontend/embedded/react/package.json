{
    "name": "@bytechef-embedded/react",
    "version": "0.1.0",
    "description": "ByteChef Embedded React SDK.",
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsc && mkdir -p dist/assets && cp -r src/assets/* dist/assets/",
        "test": "jest",
        "test:watch": "jest --watch",
        "lint": "eslint 'src/**/*.{ts,tsx}'",
        "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix",
        "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md}\" && eslint src --max-warnings=0 --fix",
        "prepare": "npm run build",
        "watch": "tsc --watch"
    },
    "keywords": [
        "react",
        "hook",
        "bytechef",
        "connect-dialog"
    ],
    "author": "Kresimir Coko <kresimir.coko@bytechef.io>",
    "license": "MIT",
    "devDependencies": {
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/react": "^16.3.0",
        "@types/jest": "^29.5.14",
        "@types/react": "^18.0.0",
        "@typescript-eslint/eslint-plugin": "^8.33.0",
        "@typescript-eslint/parser": "^8.33.0",
        "babel-jest": "^29.7.0",
        "concurrently": "^7.0.0",
        "eslint": "^8.57.1",
        "eslint-config-prettier": "^10.1.5",
        "eslint-plugin-prettier": "^5.4.1",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-sort-destructure-keys": "^2.0.0",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^30.0.0-beta.3",
        "prettier": "^3.5.3",
        "react": "^19.1.0",
        "ts-jest": "^29.3.4",
        "typescript": "^5.0.0"
    },
    "peerDependencies": {
        "react": ">=18.0.0"
    },
    "dependencies": {
    }
}
