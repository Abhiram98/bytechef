{
    "name": "@bytechef/use-embedded-connection-dialog",
    "version": "0.1.0",
    "description": "A React hook for managing embedded ByteChef connection dialogs.",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "style": "dist/styles.css",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "npx tailwindcss -i src/styles.css -o dist/styles.css && tsc && mkdir -p dist/assets && cp -r src/assets/* dist/assets/",
        "test": "jest",
        "test:watch": "jest --watch",
        "lint": "eslint 'src/**/*.{ts,tsx}'",
        "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix",
        "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md}\" && eslint src --max-warnings=0 --fix",
        "prepare": "npm run build",
        "watch": "concurrently \"npx tailwindcss -i src/styles.css -o dist/styles.css --watch\" \"tsc --watch\""
    },
    "keywords": [
        "react",
        "hook",
        "bytechef",
        "connection-dialog"
    ],
    "author": "Your Name <your.email@example.com>",
    "license": "MIT",
    "devDependencies": {
        "@tailwindcss/forms": "^0.5.10",
        "@tailwindcss/typography": "^0.5.16",
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/react": "^16.3.0",
        "@types/jest": "^29.5.14",
        "@types/react": "^18.0.0",
        "@typescript-eslint/eslint-plugin": "^8.33.0",
        "@typescript-eslint/parser": "^8.33.0",
        "autoprefixer": "^10.4.21",
        "babel-jest": "^29.7.0",
        "concurrently": "^7.0.0",
        "eslint": "^8.57.1",
        "eslint-config-prettier": "^10.1.5",
        "eslint-plugin-prettier": "^5.4.1",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-sort-destructure-keys": "^2.0.0",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^30.0.0-beta.3",
        "lucide-react": "^0.511.0",
        "postcss": "^8.5.4",
        "postcss-cli": "^11.0.1",
        "prettier": "^3.5.3",
        "prettier-plugin-tailwindcss": "^0.6.11",
        "react": "^19.1.0",
        "tailwindcss": "^3.4.17",
        "tailwindcss-animate": "^1.0.7",
        "ts-jest": "^29.3.4",
        "typescript": "^5.0.0"
    },
    "peerDependencies": {
        "react": ">=18.0.0"
    },
    "dependencies": {
        "@hookform/resolvers": "^5.0.1",
        "react-hook-form": "^7.57.0",
        "zod": "^3.25.49"
    }
}
